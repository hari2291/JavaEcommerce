<div class="box">
	<div class="icon-holder small badge-style">
		<i class="fa fa-check-circle-o "></i> <span class="triangle"></span>
	</div>
	<div id="existingAddress">
		<div class="box-holder">
			Select shipping address:

			<div class="radio">
				<label><input type="radio" value="opt1" name="optradio">Option
					1</label>
			</div>
			<div class="radio">
				<label><input type="radio" value="opt2" name="optradio">Option
					2</label>
			</div>
			<div class="radio">
				<label><input type="radio" value="opt3" name="optradio">Option
					3</label>
			</div>
		</div>
	</div>
	<div id="newAddress">
		<div class="box-holder">
			Enter your shipping address.
			<div class="miniblankrow"></div>
			<div class="row">
				<div class="col-xs-12 col-md-12">

					<div class="table-form ">
						<div class="row">
							<div class="col-xs-12 col-md-6">
								<div class="field-group">
									<input type="text" class="le-input" id="address1"
										data-placeholder="Flat/Building Details">
								</div>
							</div>

							<div class="col-xs-12 col-md-6">
								<div class="field-group">
									<input type="text" class="le-input" id="address2"
										data-placeholder="Street/Road">
								</div>
							</div>
						</div>
						<div class="row">
							<div class="col-xs-12 col-md-6">
								<div class="field-group">
									<input type="text" class="le-input" id="landmark"
										data-placeholder="Landmark">
								</div>
							</div>
							<div class="col-xs-12 col-md-6">
								<div class="field-group">
									<input type="text" class="le-input" id="city"
										data-placeholder="City">
								</div>
							</div>

						</div>
						<div class="row">
							<div class="col-xs-12 col-md-6">
								<div class="field-group">
									<input type="text" class="le-input" data-placeholder="State"
										id="state">
								</div>
							</div>
							<div class="col-xs-12 col-md-6">
								<div class="field-group">
									<input type="text" class="le-input" id="pincode"
										data-placeholder="Pincode">
								</div>
							</div>

						</div>
					</div>
				</div>


			</div>

		</div>

	</div>
	<div class="col-xs-12 col-md-12">
		<div class="table-form ">
			<div class="row">
				<div class="col-xs-12 col-md-12 centerAligned">
					<div class="field-group">
						<a href="javascript:createAddress();" class="le-btn small">Payment&nbsp;&gt;&gt;</a>
					</div>
				</div>
			</div>

		</div>
	</div>
</div>