<!DOCTYPE html>

<div class="row">
	<div class="col-xs-4 col-md-4"></div>
	<div class="box col-xs-4 col-md-4">
		<div class="icon-holder small badge-style">
			<i class="fa fa-check-circle-o "></i> <span class="triangle"></span>
		</div>
		<div class="box-holder">

			<div class="row">

				<div class="col-xs-12 col-md-12">
					<h2>Forgot Password</h2>
					<div class="table-form ">
						<div id="forgotPasswordSuccess">Email sent with
							instruction to reset the password</div>
						<div class="row">
							<div class="col-xs-12 col-md-12">
								<div class="field-group">
									<input type="text" id="loginEmailId"
										class="le-input placeholder-field"> <label
										alt='Placeholder' placeholder='Email Id'></label>
								</div>
							</div>
						</div>

						<div class="row">
							<div class="col-xs-12 col-md-12">
								<div class="field-group rightAligned">
									<a href="javascript:forgotPassword();" class="le-btn small">Reset
										Password</a>
								</div>
							</div>
						</div>

					</div>
				</div>
			</div>

		</div>

	</div>
	<div class="col-xs-4 col-md-4"></div>
</div>
<div class="blankrow"></div>